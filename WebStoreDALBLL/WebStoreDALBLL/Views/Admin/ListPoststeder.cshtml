@model List<WebStoreDALBLL.Models.Poststed>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model[0].postnr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model[0].poststed)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.postnr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.poststed)
        </td>
        <td>
            @Html.ActionLink("Detaljer", "DetailsPoststeder", new { id=item.postnr }) |
            @Html.ActionLink("Delete", "DeletePoststed", new { id=item.postnr })
        </td>
    </tr>
}

</table>
